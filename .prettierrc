{
    "singleQuote": true,
    "semi": true,
    "printWidth": 100,
    "tabWidth": 4,
    "plugins": ["prettier-plugin-astro", "prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        },
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
